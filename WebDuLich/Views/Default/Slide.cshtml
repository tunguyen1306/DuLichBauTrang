@using System.Configuration
@model IEnumerable<WebDuLich.Models.tbl_slide_dulich>
<div id="header_wrapper" class="header_wrapper">
    <div class="container-12">
        <div class="grid-12">
            <div class="region region-header">
                <div id="block-views-slider-block" class="block-views-slider-block block block-views block-even">


                    <div class="content block-content">
                        <div class="view view-slider view-id-slider view-display-id-block view-dom-id-c942f2c822f21042195b5f95b7788c0b">
                            <div class="view-content">
                                <div id="flexslider-1" class="flexslider">
                                    <ul class="slides">
                                        @foreach (var item in Model)
                                        {
                                            <li>
                                                <div class="views-field views-field-field-slide-image">
                                                    <div class="field-content">
                                                        <img src="@ConfigurationManager.AppSettings["domain"]@item.img_silde_dulich.ToString().Split('_')[0]/@item.img_silde_dulich"   />
                                                    </div>
                                                </div>
                                                <div class="flex-caption">
                                                    <h2>
                                                        <p>
                                                            <span style="color: #f27a8e;">@item.name_silde_dulich</span>
                                                            
                                                        </p>
                                                    </h2>@item.tomtat_silde_dulich
                                                </div>
                                            </li>
                                        }
                                       
                                       
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
